{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "dev-A": "concurrently \"rollup -c -w\" \"firebase emulators:start --only hosting\"",
    "emulate": "firebase emulators:start --only=database,functions",
    "dev:emulate": "firebase emulators:start",
    "dev:RollUp": "rollup -c -w",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "concurrently": "^6.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "firebase": "^8.6.8",
    "firebase-admin": "^9.10.0",
    "sirv-cli": "^1.0.0",
    "svelte-notifications": "^0.9.91"
  }
}
